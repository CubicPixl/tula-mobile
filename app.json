{
  "expo": {
    "name": "Tula Turismo",
    "slug": "tula-mobile",
    "version": "0.1.0",
    "sdkVersion": "52.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "tula",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [],
      "config": {
        "googleMaps": {
          "apiKey": "PASTE_YOUR_ANDROID_MAPS_KEY_HERE"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "server"
    },
    "plugins": [],
    "extra": {
      "eas": {
        "projectId": "local-dev"
      },
      "apiUrl": "http://localhost:4000"
    }
  }
}